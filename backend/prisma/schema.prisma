datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id            Int      @id @default(autoincrement())
  nome          String?
  email         String?  @unique
  senha         String?
  status        Boolean  @default(false)      // <-- aqui
  data_registro DateTime @default(now())

  @@map("usuario")
}